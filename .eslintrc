{
  "extends": [
    "react-app",
    "prettier" 
  ],
  "rules": {
    "jsx-quotes": [
      1,
      "prefer-double"
    ],
    "jsx-a11y/anchor-is-valid": "disable",
    "prettier/prettier": ["error", {
      "singleQuote": true,
      "trailingComma": "all"
    }]
  },
  "plugins": [
    "prettier"
  ]
}